module Servactory
  module Outputs
    module Checks
      class Type < Base
        DEFAULT_MESSAGE: Proc

        @context: instance

        @output: Output
        @prepared_types: Array[Class]
        @value: untyped

        def self.check!: (*untyped) ?{ () -> untyped } -> void

        def initialize: (context: instance, output: Output, value: untyped) -> void

        def check!: () -> void

        private

        def prepared_types: -> Array[Class]
      end
    end
  end
end
