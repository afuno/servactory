module Servactory
  class Result
    def self.success_for: (*untyped) ?{ () -> untyped } -> Result

    def self.failure_for: (*untyped) ?{ () -> untyped } -> Result

    private

    def success_for: (context: instance, collection_of_outputs: Outputs::Collection) -> self

    def failure_for: (exception: Exception) -> self

    def prepare_outputs_with: (context: instance, collection_of_outputs: Outputs::Collection) -> void
  end
end
